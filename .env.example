# Amadeus Self-Service API (https://developers.amadeus.com)
# Get free credentials at https://developers.amadeus.com/self-service
AMADEUS_CLIENT_ID=your_amadeus_client_id
AMADEUS_CLIENT_SECRET=your_amadeus_client_secret

# Amadeus environment: "test" or "production"
AMADEUS_ENV=test

# Set to "true" to use mock data (no Amadeus account needed)
AMADEUS_MOCK_MODE=false

# Vite frontend: set to "true" to use built-in mock data (no backend needed)
VITE_MOCK_API=true

# Optional: MapTiler API key for premium map styles
# Get a free key at https://maptiler.com
# VITE_MAPTILER_KEY=your_maptiler_key
